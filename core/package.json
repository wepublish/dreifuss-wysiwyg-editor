{
  "name": "@dreifuss-plugins/core",
  "version": "0.0.1",
  "description": "A React Component generated by starter-react-component ( https://www.npmjs.com/package/starter-react-component ).",
  "main": "./lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.demo.config.ts --progress --mode development --hot",
    "build-demo": "webpack --config webpack.demo.config.ts --progress --mode production",
    "build-component": "webpack --progress --mode production",
    "build": "npm run build-component && npm run build-demo",
    "prepublish": "npm run build",
    "test": "BABEL_JEST_STAGE=0 jest",
    "lint": "eslint 'src/**/*.{ts,tsx,js}' '__tests__/**/*.{ts,tsx,js}'",
    "fix": "eslint 'src/**/*.{ts,tsx,js}' '__tests__/**/*.{ts,tsx,js}' --fix"
  },
  "keywords": [
    "rich-text",
    "slate",
    "publishing",
    "media",
    "newspaper",
    "cms",
    "subscription",
    "graphql"
  ],
  "author": "Nicolas Roos <nicolas.k.roos@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/flux": "^3.1.9",
    "@types/jest": "^26.0.22",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.4",
    "jest-cli": "^26.6.3",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.4",
    "webpack": "^5.31.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-bundle-size-analyzer": "^3.1.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@udecode/slate-plugins": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-alignment": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-alignment-ui": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-autoformat": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-basic-elements": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-basic-marks": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-block-quote": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-block-quote-ui": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-break": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-code-block": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-code-block-ui": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-common": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-core": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-find-replace": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-find-replace-ui": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-heading": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-highlight": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-html-serializer": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-image": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-image-ui": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-kbd": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-link": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-link-ui": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-list": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-list-ui": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-md-serializer": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-media-embed": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-media-embed-ui": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-mention": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-mention-ui": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-select": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-table": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-table-ui": "^1.0.0-alpha.25",
    "@udecode/slate-plugins-trailing-block": "^1.0.0-alpha.25",
    "emoji-mart": "^3.0.1",
    "jsx-runtime": "^1.2.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "slate": "^0.62.0",
    "slate-history": "^0.62.0",
    "slate-hyperscript": "^0.62.0",
    "slate-react": "^0.62.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "eslintConfig": {
    "extends": [
      "../../package.json"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    },
    "rules": {
      "react/display-name": [
        0
      ],
      "react/prop-types": [
        0
      ],
      "@typescript-eslint/explicit-function-return-type": "off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".ts",
            ".tsx"
          ]
        }
      ],
      "import/prefer-default-export": "off",
      "react/no-find-dom-node": 0,
      "class-methods-use-this": "off",
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to",
            "hrefLeft",
            "hrefRight"
          ],
          "aspects": [
            "noHref",
            "invalidHref",
            "preferButton"
          ]
        }
      ],
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ]
    }
  }
}
