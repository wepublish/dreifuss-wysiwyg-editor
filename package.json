    "@testing-library/user-event": "^12.7.3",
    "@types/faker": "^5.1.7",
    "@types/is-hotkey": "^0.1.2",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/marked": "^1.2.2",
